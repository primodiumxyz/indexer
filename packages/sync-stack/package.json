{
  "name": "@primodiumxyz/sync-stack",
  "version": "0.6.8",
  "description": "Modularized sync-stack heavily based on Lattice's MUD sync-store for modified custom indexer front-end.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primodiumxyz/mud-state-tools",
    "directory": "./packages/sync-stack"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup",
    "clean": "pnpm run clean:js",
    "clean:js": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "@latticexyz/block-logs-stream": "2.0.9",
    "@latticexyz/common": "2.0.9",
    "@latticexyz/protocol-parser": "2.0.9",
    "@latticexyz/recs": "2.0.9",
    "@latticexyz/schema-type": "2.0.9",
    "@latticexyz/store": "2.0.9",
    "debug": "^4.3.5",
    "eventemitter3": "^5.0.1",
    "viem": "2.9.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^18.19.34",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "tsx": "^3.14.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
